							<script type='text/javascript'>
								const radioc = document.querySelectorAll(".imgd");
								const desmarcar = document.querySelector("#img");
								
								radioc.forEach(r => {
									r.addEventListener('click', () => {
										desmarcar.value = r.value
									});
								});
								
								desmarcar.addEventListener('change', () => {
									radioc.forEach((input) => {
										input.checked = false;
									})
								});
