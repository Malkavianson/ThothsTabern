<div id='registerBox'>
	<div>
		<h1>Cadastre-se</h1>
	</div>
	<div>
		<form id='register' action='/register' method='POST'>
			<fieldset form='register' name='register'>
				<legend>Registre sua conta</legend>
					<div>
						<label for='user'>Username</label>
						<input class='input' minlength='1' maxlength='10' autocomplete='auto' type='text' name='user' placeholder='username' required>
						<label for='user'>Nome</label>
						<input class='input' autocomplete='auto' minlength='1' maxlength='15' type='text' name='name' placeholder='seu primeiro nome' required>
						<label for='pw'>Senha</label>
						<input class='input' minlength='1' maxlength='10' autocomplete='current-password' type='password' name='pw' placeholder='*****' required>
					</div>
					<div>
						<button type='submit'>Registrar</button>
					</div>
			</fieldset>
		</form>
	</div>
	<div class='change' onclick="change('l')">
		<span>Login</span>
	</div>
</div>